{
  "expo": {
    "name": "plus_life",
    "slug": "plus_life",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon_app.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "plus_life",
    "splash": {
      "image": "./assets/expo_assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/expo_assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/expo_assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ]
  }
}
